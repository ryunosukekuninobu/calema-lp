{
    "name": "calema/lp",
    "description": "Landing page module for Laravel applications with multi-tenancy support",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "landing-page", "lp", "multi-tenancy", "saas"],
    "authors": [
        {
            "name": "Calema Team",
            "email": "dev@calema.tech"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Calema\\Lp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Calema\\Lp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Calema\\Lp\\LandingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
